{
  "data-dir": "./data/custom_dataset",
  "anno-file": "./data/custom_dataset/annotations.csv",
  "img-size": 256,
  "batch-size": 64,
  "model-type": "small",
  "num-classes": 20,
  "epochs": 200,
  "lr": 5e-4,
  "optimizer": "adamw",
  "scheduler": "cosine",
  "early-stopping": true,
  "patience": 15,
  "log-dir": "./custom_logs",
  "checkpoint-dir": "./custom_checkpoints",
  "experiment-name": "config_file_test",
  
  "loss-type": "cross_entropy",
  "t-max": 200,
  "eta-min": 1e-6,
  "min-lr": 1e-6,
  "beta1": 0.9,
  "beta2": 0.999,
  "eps": 1e-8,
  "weight-decay": 1e-4
} 